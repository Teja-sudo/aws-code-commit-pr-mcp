{
  "name": "@tejasanik/aws-pr-reviewer",
  "version": "1.3.0",
  "description": "A Model Context Protocol (MCP) server for AWS code commit PR management",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aws-pr-reviewer": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "nodemon --exec tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-and-start": "npm run build && npm start",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "aws",
    "codecommit",
    "pull-request",
    "code-review",
    "ai"
  ],
  "author": "Teja",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.2.1",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@aws-sdk/client-codecommit": "^3.864.0",
    "@aws-sdk/credential-providers": "^3.864.0",
    "@modelcontextprotocol/sdk": "^1.17.2",
    "@types/diff": "^7.0.2",
    "@types/treeify": "^1.0.3",
    "diff": "^8.0.2",
    "treeify": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Teja-sudo/aws-code-commit-pr-mcp.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
