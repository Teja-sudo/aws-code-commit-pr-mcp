# AWS Configuration
AWS_PROFILE=dev
AWS_DEFAULT_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_SESSION_TOKEN=

# Application Configuration
LOG_LEVEL=INFO
SERVER_NAME=aws-pr-review
SERVER_VERSION=1.0.0

# Performance Configuration
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT_SECONDS=30

# Optional: Redis Configuration (if using cache profile)
REDIS_PASSWORD=changeme

# Optional: PostgreSQL Configuration (if using database profile)
POSTGRES_DB=aws_pr_mcp
POSTGRES_USER=mcpuser
POSTGRES_PASSWORD=changeme

# Optional: Nginx Configuration (if using proxy profile)
HTTP_PORT=80
HTTPS_PORT=443